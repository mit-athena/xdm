 */

WCL=wcl
XDM=xdm
DM=dm

#ifdef sgi
WCL=
XDM=
DM=
#define WSUBDIRS wcl.r5
#endif

#ifdef SOLARIS
XDM=
#endif

#define ISUBDIRS ${WCL} ${DM} ${XDM} xlogin console conf 


/* In the imake template, ISUBDIRS is all built before XSUBDIRS,
   but we need wcl.r5 to be built before xlogin. We also don't care
   to install it. */
#ifdef WSUBDIRS
do_subdirs_xmakefile(WSUBDIRS)
foreach_subdirs(all,WSUBDIRS)
foreach_subdirs(clean,WSUBDIRS)
foreach_subdirs(depend,WSUBDIRS)
#endif
