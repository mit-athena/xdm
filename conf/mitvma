#!/bin/sh

PATH=/etc:/usr/ucb:/bin:/usr/bin:.:/etc/athena:/bin/athena:/usr/bin/athena
export PATH
SHELL=/bin/echo
export SHELL

attach -n -h -q mitvma

MAP3270=/mit/mitvma/map3270; export MAP3270

/usr/bin/X11/xterm -geometry 80x34+60+25 +ut -fn -adobe-courier-medium-r-normal--18-180-75-75-\*-\*-\*-\* -display :0.0 -e /usr/athena/timeout 120 /usr/ucb/tn3270 mitvma.mit.edu

detach -q mitvma

exit 0
