error_table(pc_err)

NANNY		=	nanny

OBJS		=	pc_err.o nanny.o pc_bsd.o cons_sysv.o

CFILES		=	pc_err.c nanny.c pc_bsd.c cons_sysv.c

LIBS		=	-L/source/athena/lib/AL -lAL -lcom_err -lhesiod

DEFINES		=	-I/source/athena/lib

AnsiCC()

SimpleProgram(${NANNY},${OBJS},${LIBS},${ATHRETCDIR}/${NANNY})
install_man(${NANNY}.man,${NANNY}.1)

create_depend(${CFILES})
