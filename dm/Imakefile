#ifdef SOLARIS
MACHLIBS = -lbsd
#endif
AFSLIBS = -L$(AFSDIR)/lib -L$(AFSDIR)/lib/afs -lprot -lauth -lrxkad \
	-lubik $(AFSDIR)/lib/afs/util.a -laudit -lsys -lrx -llwp -lsys
LIBS = -lX11 -lal -llocker $(AFSLIBS) -lzephyr -lkrb4 -ldes425 -lkrb5 \
	-lcrypto -lcom_err -lhesiod $(MACHLIBS)

SimpleProgram(dm,dm.o,$(LIBS),/etc/athena)
install_man(dm.8,dm.8)

